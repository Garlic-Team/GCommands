{
  "name": "gcommands",
  "version": "9.2.0",
  "description": "Powerful and flexible framework!",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./*.js",
      "require": "./*.js"
    }
  },
  "scripts": {
    "build": "bash scripts/build.bash",
    "lint": "npx eslint .",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s",
    "build:tsc": "npx tsc -p . && gen-esm-wrapper dist/index.js dist/index.mjs",
    "build:fix": "node scripts/fixer.mjs",
    "build:docs": "typedoc --json docs/out-latest.json src/index.ts && node scripts/docs.mjs",
    "build:nextdocs": "typedoc --json docs/out-next.json src/index.ts && node scripts/next-docs.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Garlic-Team/GCommands.git"
  },
  "bugs": {
    "url": "https://github.com/Garlic-Team/GCommands/issues"
  },
  "homepage": "https://garlic-team.js.org/#/docs/gcommands/next/general/welcome",
  "author": "Garlic-Team",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "discord-api-types": "^0.26.1",
    "ms": "^2.1.3",
    "tslib": "^2.3.1",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@discordjs/ts-docgen": "0.3.4",
    "@gcommands/plugin-language": "1.0.4",
    "@google-cloud/firestore": "5.0.2",
    "@octokit/action": "3.18.0",
    "@prisma/client": "3.12.0",
    "@types/keyv": "3.1.4",
    "@types/lru-cache": "7.6.1",
    "@types/node": "16.11.26",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "conventional-changelog-cli": "2.2.2",
    "discord.js": "13.6.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "gen-esm-wrapper": "1.1.3",
    "keyv": "4.2.1",
    "lru-cache": "7.7.3",
    "mongodb": "4.5.0",
    "prettier": "2.6.2",
    "typedoc-plugin-djs-links": "1.0.4",
    "typescript": "4.6.3"
  },
  "keywords": [
    "discord.js",
    "framework",
    "gcommands",
    "garlic-team",
    "typescript",
    "javascript",
    "discord",
    "bot"
  ],
  "engines": {
    "node": ">=16.6"
  }
}
