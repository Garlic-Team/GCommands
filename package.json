{
  "name": "gcommands",
  "version": "8.0.0-dev",
  "description": "Powerful and flexible command handler that can do everything!",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@gcommands/components": "^1.0.3",
    "@gcommands/events": "^2.0.6",
    "@keyvhq/core": "^1.6.0",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.2.0",
    "hyttpo": "^0.2.6",
    "ms": "^2.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@discordjs/ts-docgen": "^0.3.2",
    "@types/node": "^16.3.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.1",
    "glob": "^7.2.0",
    "typescript": "^4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/garlic-team/gcommands.git"
  },
  "keywords": [
    "gcommands",
    "handler",
    "slash commands",
    "context menus"
  ],
  "author": "Garlic-Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Garlic-Team/GCommands/issues"
  },
  "scripts": {
    "build": "bash build.bash",
    "test": "eslint src",
    "babelbuild": "npx babel --extensions .ts ./src -d ./dist --minified --copy-files",
    "tscbuild": "tsc --emitDeclarationOnly",
    "docs:build": "typedoc --json docs/out-main.json src/index.ts && node scripts/docs.mjs",
    "docs:devbuild": "typedoc --json docs/out-dev.json src/index.ts && node scripts/devdocs.mjs"
  },
  "engines": {
    "node": ">=16.6.0"
  },
  "homepage": "https://gcommands.js.org"
}
