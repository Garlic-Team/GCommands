{
  "name": "gcommands",
  "version": "0.1.0-nextdev",
  "description": "Powerful and flexible framework!",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bash scripts/build.bash",
    "lint": "npx eslint .",
    "build:babel": "npx babel --extensions .ts ./src -d ./dist --minified --copy-files",
    "build:types": "tsc --emitDeclarationOnly",
    "build:docs": "typedoc --json docs/out-next.json src/index.ts && node scripts/docs.mjs",
    "build:devdocs": "typedoc --json docs/out-dev.json src/index.ts && node scripts/devdocs.mjs"
  },
  "homepage": "https://garlic-team.js.org/#/docs/gcommands/next/general/welcome",
  "author": "Garlic-Team",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.5.0",
    "js-logger": "^1.6.1",
    "ms": "^2.1.3"
  },
  "devDependencies": {
    "@babel/cli": "7.16.7",
    "@babel/core": "7.16.7",
    "@babel/preset-env": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@types/node": "16.11.19",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "babel-cli": "6.26.0",
    "babel-plugin-add-module-exports": "1.0.4",
    "eslint": "8.6.0",
    "typescript": "4.5.4"
  },
  "engines": {
    "node": ">=16.6"
  }
}
